{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier -l \"./src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "format:fix": "yarn format --write",
    "preview": "vite preview",
    "gen:gql": "graphql-codegen --config codegen.ts --watch --silent",
    "gen:routes": "g-routes --input ./src/pages --output ./src/generated",
    "prefire:deploy": "yarn build",
    "fire:deploy": "firebase deploy",
    "dev:all": "concurrently -n \"dev,gql\" \"yarn dev\" \"yarn gen:gql\""
  },
  "dependencies": {
    "@apollo/client": "^3.7.12",
    "@chakra-ui/react": "^2.6.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "browser-image-compression": "^2.0.2",
    "chakra-react-select": "^4.6.0",
    "firebase": "^9.20.0",
    "framer-motion": "^10.12.4",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.10.0",
    "react-use": "^17.4.0",
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/client-preset": "^3.0.1",
    "@sogaw/g-routes": "^0.0.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
